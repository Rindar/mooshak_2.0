@model List<mooshak_2._0.Models.ViewModels.AssignmentViewModel>
@{
    ViewBag.Title = "Assignment";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

<h2>Assignment</h2>



<table class="table table-bordered table-condensed">
    <thead>
        <tr>
            <th style="text-align: center;">Assignment</th>
            <th style="text-align: center;">Assignment ID<th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td style="text-align: center;">@Html.DisplayFor(modelItem => item.title)</td>
                <td style="text-align: center;">@Html.DisplayFor(modelItem => item.id)</td>
            </tr>
        }
    </tbody>
</table>