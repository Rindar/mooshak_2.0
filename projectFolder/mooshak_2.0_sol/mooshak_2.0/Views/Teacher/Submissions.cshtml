@model IEnumerable<mooshak_2._0.Models.Entities.Submissions>
@{
    ViewBag.Title = "Submissions";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Submissions</h2>


<table class="table table-bordered table-condensed">
    <thead>
        <tr>
            <th style="text-align: center;">User</th>
            <th style="text-align: center;">Solution Name</th>
            <th style="text-align: center;">Download Solution</th>


        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
            {
            <tr>
                <td style="text-align: center;">@Html.DisplayFor(modelItem => item.UserName)</td>
                <td style="text-align: center;">@Html.DisplayFor(modelItem => item.FileName)</td>
                <td style="text-align: center;"><a href="/Teacher/GetFile/@item.Id">Download</a></td>
            </tr>
        }
    </tbody>


</table>
